class Solution(object):
    def isValid(self, s):
        """
        :type s: str
        :rtype: bool
        """
        bank = {
            '(': ')',
            '[': ']',
            '{': '}'
        }

        call_stack = []

        for i in s:
            if i in bank:
                call_stack.append(bank[i])
            else:
                if len(call_stack)==0:
                    return False
                elif i == call_stack[len(call_stack)-1]:
                    call_stack = call_stack[0:len(call_stack)-1]
                else:
                    return False

        
        if len(call_stack)>0:
            return False

        return True
